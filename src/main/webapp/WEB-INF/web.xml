<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UsuarioController</servlet-name>
        <servlet-class>com.unsis.capcr.controller.UsuarioController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuarioController</servlet-name>
        <url-pattern>/UsuarioController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AlumnosController</servlet-name>
        <servlet-class>com.unsis.capcr.controller.AlumnosController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AlumnosController</servlet-name>
        <url-pattern>/AlumnosController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PracticaController</servlet-name>
        <servlet-class>com.unsis.capcr.controller.PracticaController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PracticaController</servlet-name>
        <url-pattern>/PracticaController</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ReservacionController</servlet-name>
        <servlet-class>com.unsis.capcr.controller.ReservacionController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReservacionController</servlet-name>
        <url-pattern>/ReservacionController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
